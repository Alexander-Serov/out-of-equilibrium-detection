4613419e8c13"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"detImage", "=", 
   RowBox[{
    RowBox[{"U", ".", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"K", "[", "#", "]"}], "&"}], ",", "\[Lambda]s"}], "]"}], 
      "]"}], ".", "Um1", ".", "R0"}], "+", 
    RowBox[{"U", ".", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"L", "[", "#", "]"}], "&"}], ",", "\[Lambda]s"}], "]"}], 
      "]"}], ".", "Um1", ".", "a"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LKImages", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"K", "[", "\[Lambda]1_", "]"}], "/;", 
       RowBox[{"\[Lambda]1", "===", "0"}]}], "\[RuleDelayed]", "0"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"K", "[", "\[Lambda]1_", "]"}], "/;", 
       RowBox[{"\[Lambda]1", "=!=", "0"}]}], "\[RuleDelayed]", 
      FractionBox[
       RowBox[{"dt", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"Exp", "[", 
           RowBox[{"M", " ", "\[Lambda]1", " ", "dt"}], "]"}]}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Exp", "[", "\[Lambda]1dt", "]"}], "-", "1"}], ")"}]}], 
       RowBox[{"1", "-", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"\[Lambda]1", " ", "dt"}], "-", 
          RowBox[{"2", " ", "\[Pi]", " ", "\[ImaginaryI]", " ", 
           RowBox[{"k", " ", "/", "M"}]}]}], "]"}]}]]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"L", "[", "\[Lambda]1_", "]"}], "/;", 
       RowBox[{"\[Lambda]1", "===", "0"}]}], "\[RuleDelayed]", 
      RowBox[{"M", " ", 
       SuperscriptBox["dt", "2"], 
       RowBox[{"\[Delta]", "[", "k", "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"L", "[", "\[Lambda]1_", "]"}], "/;", 
       RowBox[{"\[Lambda]1", "=!=", "0"}]}], "\[RuleDelayed]", 
      RowBox[{
       FractionBox[
        RowBox[{"dt", " ", 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           RowBox[{"Exp", "[", 
            RowBox[{"M", " ", "\[Lambda]1", " ", "dt"}], "]"}]}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Exp", "[", "\[Lambda]1dt", "]"}], "-", "1"}], ")"}]}], 
        RowBox[{"1", "-", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"\[Lambda]1", " ", "dt"}], "-", 
           RowBox[{"2", " ", "\[Pi]", " ", "\[ImaginaryI]", " ", 
            RowBox[{"k", " ", "/", "M"}]}]}], "]"}]}]], "/", 
       "\[Lambda]1"}]}]}], "\[IndentingNewLine]", "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"detImage", "/.", "LKImages"}], "//", 
  "Simplify"}], "\[IndentingNewLine]", 
 RowBox[{"%", "//", "MatrixForm"}]}], "Input",
