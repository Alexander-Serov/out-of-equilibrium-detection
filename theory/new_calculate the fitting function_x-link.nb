   RowBox[{"{", 
    FractionBox[
     RowBox[{"dt", " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        FractionBox[
         RowBox[{"2", " ", "\[ImaginaryI]", " ", "k", " ", "\[Pi]"}], "M"], 
        "-", 
        RowBox[{"2", " ", "dt", " ", 
