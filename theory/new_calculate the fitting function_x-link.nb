Cell[CellGroupData[{

Cell["\<\
Get the image of the deterministic part. Keep only terms for L[0], because \
everything else is ~O(1)\
\>", "Subsection",
 CellChangeTimes->{{3.76674036164264*^9, 3.7667403683224497`*^9}, {
  3.7667405024827523`*^9, 
  3.76674052093925*^9}},ExpressionUUID->"9d551300-ffd5-448b-a76b-\
4613419e8c13"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"detImage", "=", 
   RowBox[{
    RowBox[{"U", ".", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"K", "[", "#", "]"}], "&"}], ",", "\[Lambda]s"}], "]"}], 
      "]"}], ".", "Um1", ".", "R0"}], "+", 
    RowBox[{"U", ".", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"L", "[", "#", "]"}], "&"}], ",", "\[Lambda]s"}], "]"}], 
      "]"}], ".", "Um1", ".", "a"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LKImages", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"K", "[", "\[Lambda]1_", "]"}], "/;", 
       RowBox[{"\[Lambda]1", "===", "0"}]}], "\[RuleDelayed]", "0"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"K", "[", "\[Lambda]1_", "]"}], "/;", 
       RowBox[{"\[Lambda]1", "=!=", "0"}]}], "\[RuleDelayed]", 
      FractionBox[
       RowBox[{"dt", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"Exp", "[", 
           RowBox[{"M", " ", "\[Lambda]1", " ", "dt"}], "]"}]}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Exp", "[", "\[Lambda]1dt", "]"}], "-", "1"}], ")"}]}], 
       RowBox[{"1", "-", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"\[Lambda]1", " ", "dt"}], "-", 
          RowBox[{"2", " ", "\[Pi]", " ", "\[ImaginaryI]", " ", 
           RowBox[{"k", " ", "/", "M"}]}]}], "]"}]}]]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"L", "[", "\[Lambda]1_", "]"}], "/;", 
       RowBox[{"\[Lambda]1", "===", "0"}]}], "\[RuleDelayed]", 
      RowBox[{"M", " ", 
       SuperscriptBox["dt", "2"], 
       RowBox[{"\[Delta]", "[", "k", "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"L", "[", "\[Lambda]1_", "]"}], "/;", 
       RowBox[{"\[Lambda]1", "=!=", "0"}]}], "\[RuleDelayed]", 
      RowBox[{
       FractionBox[
        RowBox[{"dt", " ", 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           RowBox[{"Exp", "[", 
            RowBox[{"M", " ", "\[Lambda]1", " ", "dt"}], "]"}]}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Exp", "[", "\[Lambda]1dt", "]"}], "-", "1"}], ")"}]}], 
        RowBox[{"1", "-", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"\[Lambda]1", " ", "dt"}], "-", 
           RowBox[{"2", " ", "\[Pi]", " ", "\[ImaginaryI]", " ", 
            RowBox[{"k", " ", "/", "M"}]}]}], "]"}]}]], "/", 
       "\[Lambda]1"}]}]}], "\[IndentingNewLine]", "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"detImage", "/.", "LKImages"}], "//", 
  "Simplify"}], "\[IndentingNewLine]", 
 RowBox[{"%", "//", "MatrixForm"}]}], "Input",
 CellChangeTimes->{{3.766740429955059*^9, 3.7667404807401114`*^9}, {
